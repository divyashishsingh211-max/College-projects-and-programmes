//WAP TO FIND LARGEST NUMBER AMOUNG N...
#include<stdio.h>
#include<math.h>
int main(){
    int i,n,num,max=0;
    printf("enter the value of n");
    scanf("%d",&n);
    for(i=1;i<=n;i++){
        printf("enter a number");
        scanf("%d", &num);
        if(num>max)
           max = num;
    }
printf("max=%d",max);      
return 0;
}
