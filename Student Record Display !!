#include <stdio.h>

int main() {
    int rollnumber;
    char name[100], branch[100], section[10], college[100];
    
    scanf("%d", &rollnumber);
    getchar(); 
    scanf("%[^\n]", name);
    getchar(); 
    scanf("%[^\n]", branch);
    getchar(); 
    scanf("%s", section);
    getchar(); 
    scanf("%[^\n]", college);

    printf("Roll Number: %d\n", rollnumber);
    printf("Name: %s\n", name);
    printf("Branch: %s\n", branch);
    printf("Section: %s\n", section);
    printf("College: %s\n", college);

    return 0;
}
