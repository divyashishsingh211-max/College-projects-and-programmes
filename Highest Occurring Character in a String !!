#include <stdio.h>
#include <string.h>

int main() {
    char S[1001];
    int freq[256] = {0};
    int maxCount = 0;
    char result = '\0';
    fgets(S, sizeof(S), stdin);
    S[strcspn(S, "\n")] = '\0';
    for (int i = 0; S[i] != '\0'; i++) {
        unsigned char c = S[i];
        freq[c]++;
    }
    for (int i = 0; S[i] != '\0'; i++) {
        unsigned char c = S[i];
        if (freq[c] > maxCount) {
            maxCount = freq[c];
            result = c;
        }
    }
    printf("%c occurs %d times\n", result, maxCount);

    return 0;
}
